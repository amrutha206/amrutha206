            "['Sunny' 'Warm' '?' 'Strong' '?' '?']\n",
            "\n",
            "Final General Hypotheses:\n",
            "['Sunny', '?', '?', '?', '?', '?']\n",
            "['?', 'Warm', '?', '?', '?', '?']\n"
          ]
        }
      ]
